<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.shbyun.quick_sensor.MainActivity">

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="121dp"
        android:id="@+id/gravity1" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/gravity1"
        android:layout_alignRight="@+id/gravity1"
        android:layout_alignEnd="@+id/gravity1"
        android:layout_marginTop="51dp"
        android:id="@+id/accelerometer1" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/accelerometer1"
        android:layout_alignRight="@+id/accelerometer1"
        android:layout_alignEnd="@+id/accelerometer1"
        android:layout_marginTop="60dp"
        android:id="@+id/linear1" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linear1"
        android:layout_alignRight="@+id/linear1"
        android:layout_alignEnd="@+id/linear1"
        android:layout_marginTop="52dp"
        android:id="@+id/gyroscope1" />
</RelativeLayout>
